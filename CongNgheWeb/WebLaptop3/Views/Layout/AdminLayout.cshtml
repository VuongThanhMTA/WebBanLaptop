<body>

    <header>
        <!-- TOP HEADER -->
        <div id="top-header">
            <div class="container">

                <ul class="header-links pull-left">
                    <li><a href="#"><i class="fa fa-phone"></i> +84-012-345-666</a></li>
                    <li><a href="#"><i class="fa fa-envelope-o"></i> mta@gmail.com</a></li>
                    <li><a href="#"><i class="fa fa-map-marker"></i> Hà Nội</a></li>
