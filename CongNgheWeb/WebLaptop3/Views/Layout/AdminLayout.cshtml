                @*@Html.Action("DKDNPartial","DangKyDangNhap")*@
                @{Html.RenderAction("DKDNPartial", "DangKyDangNhap");}
            </div>
        </div>
        <!-- /TOP HEADER -->
        <!-- MAIN HEADER -->
        <div id="header">
            <!-- container -->
            <div class="container">
                <!-- row -->
                <div class="row">
                    <!-- LOGO -->
                    <!-- LOGO -->
                    <div class="col-md-3">
                        <div class="header-logo">
                            <a href="#" class="logo">
                                <img src="~/Content/Admin/img/logo.png" />
                            </a>
                        </div>
                    </div>
                    <!-- /LOGO -->
                    <!-- SEARCH BAR -->
                    <div class="col-md-6">
                        <div class="header-search">
                            @using (Html.BeginForm("KetQuaTimKiem", "TimKiem", FormMethod.Post))
                            {
                                <select class="input-select" name="cbLoaiTimKiem" style="width:100px">
                                    <option value="0">Tất cả</option>
                                    <option value="1">Asus</option>
                                    <option value="2">HP</option>
                                    <option value="2">Apple</option>
                                    <option value="2">Sonny</option>
                                </select>
                                <input class="input" placeholder="Tìm kiếm tại đây" name="txtTimKiem" style="width:300px">
                                <button class="search-btn">Tìm kiếm</button>
                            }
                        </div>
                    </div>
                    <!-- /SEARCH BAR -->
                    <!-- ACCOUNT -->
                    <div class="col-md-3 clearfix">
                        <div class="header-ctn">
                            <!-- Cart -->
                            <div class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
                                    @Html.Action("GioHangPartial", "GioHang")

                                </a>
                            </div>
                            <!-- /Cart -->
                        </div>
                    </div>
                    <!-- /ACCOUNT -->
                </div>
                <!-- row -->
            </div>
            <!-- container -->
        </div>
        <!-- /MAIN HEADER -->
    </header>
    <!-- /HEADER -->
    <!-- NAVIGATION -->
    <nav id="navigation">
        <!-- container -->
        <div class="container">
            <!-- responsive-nav -->
            <div id="responsive-nav">
                <!-- NAV -->
                <ul class="main-nav nav navbar-nav">
                    <li class="default">@Html.ActionLink("Trang chủ", "Index", "Home")</li>
                    <li class="default">@Html.ActionLink("Tất cả", "AllLaptopPartial", "Laptop")</li>
                    <li><a href="#">Giới thiệu</a></li>
                    <li><a href="#">Liên hệ</a></li>
                    <li><a href="#">Hướng dẫn</a></li>
                    <li class="active">@Html.ActionLink("Trang quản lý", "Index", "Quanlysanpham")</li>
                </ul>
                <!-- /NAV -->
            </div>
            <!-- /responsive-nav -->
        </div>
